<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="com.dev.anhnd.android_note_todos.ui.main.MainViewModel" />

        <import type="com.dev.anhnd.android_note_todos.ui.create_note.CreateNoteViewModel" />

        <variable
            name="mainViewModel"
            type="MainViewModel" />

        <variable
            name="viewModelF"
            type="CreateNoteViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.AppCompatTextView
            android:text="@{mainViewModel.note.title}"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>